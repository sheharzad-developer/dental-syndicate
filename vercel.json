{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/static/css/(.*)", "dest": "/static/css/$1" },
    { "src": "/static/js/(.*)", "dest": "/static/js/$1" },
    { "src": "/static/images/(.*)", "dest": "/static/images/$1" },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/api/index.py" }
  ]
}